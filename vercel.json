{
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		},
		{
			"src": "dist/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js"
		},
		{
			"src": "/books",
			"dest": "index.js"
		},
		{
			"src": "/books/(.*)",
			"dest": "index.js"
		},
		{
			"src": "/books/(.*)",
			"dest": "dist/index.html"
		},
		{
			"src": "/books/",
			"dest": "dist/index.html"
		},
		{
			"src": "/",
			"dest": "dist/index.html"
		}
	]
}
